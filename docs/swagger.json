            "type": "object",
            "required": [
                "content",
                "title"
            ],
            "properties": {
                "content": {
                    "type": "string",
                    "maxLength": 40000,
                    "minLength": 1
                },
                "isDraft": {
                    "description": "true = save as draft (for video encoding)",
